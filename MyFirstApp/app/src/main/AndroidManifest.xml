    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >

        <activity
            android:name="com.example.user.myfirstapp.MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.user.myfirstapp.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.user.myfirstapp.MyActivity"
            android:theme="@style/Theme.AppCompat.Light" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.myfirstapp.MyActivity" />
        </activity>

        <activity
            android:name="com.example.user.myfirstapp.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.example.user.myfirstapp.MyActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.myfirstapp.MyActivity" />
        </activity>

        <activity
            android:name="com.example.user.myfirstapp.HelpActivity"
            android:label="@string/title_activity_help"
            android:parentActivityName="com.example.user.myfirstapp.MyActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.user.myfirstapp.MyActivity" />
        </activity>
    </application>

</manifest>
